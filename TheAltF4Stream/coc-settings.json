{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "graphql",
    "go",
    "javascript",
    "javascriptreact",
    "json",
    "less",
    "markdown",
    "rust",
    "scss",
    "typescript",
    "typescriptreact"
  ],
  "languageserver": {
    "go": {
      "command": "gopls",
      "filetypes": ["go"],
      "rootPatterns": ["go.mod"]
    },
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {},
      "settings": {}
    }
  },
  "Lua.diagnostics.globals": ["vim"]
}
